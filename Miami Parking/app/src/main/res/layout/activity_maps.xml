<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MapsActivity" >

    <Button
        android:id="@+id/toggleListID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:backgroundTint="@color/common_google_signin_btn_text_light_focused"
        android:text="Toggle List"
        android:visibility="gone" />

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MapsActivity" />

    <ImageButton
        android:id="@+id/LocaitonFinderID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:src="@android:drawable/ic_menu_mylocation"
        android:tint="@android:color/transparent"
        android:visibility="gone" />


    <ListView
        android:id="@+id/listviewID"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:entries="@mipmap/ic_launcher"
        android:paddingBottom="45dp"
        android:paddingTop="45dp"
        android:scrollbars="vertical"
        android:visibility="gone"
        android:background="@color/white"/>

    <Button
        android:id="@+id/ParkingLotDetail"
        android:layout_width="129dp"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:text="Parking Lot Detail"
        android:visibility="gone" />


    <ImageButton
        android:id="@+id/search_PL"
        android:layout_width="45dp"
        android:layout_height="39dp"
        android:layout_gravity="top|left"
        android:src="@android:drawable/ic_search_category_default"
        android:text="Search Parking Lot" />

</FrameLayout>

